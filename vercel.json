{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/list-prompts",
      "dest": "/api/list_prompts.py"
    },
    {
      "src": "/api/health",
      "dest": "/api/index.py"
    },
    {
      "src": "/api/generate-prompt",
      "dest": "/api/index.py"
    },
    {
      "src": "/api/process-prompt",
      "dest": "/api/index.py"
    },
    {
      "src": "/((?!api/|assets/|.*\\..*).*)",
      "dest": "/index.html"
    }
  ],
  "framework": null,
  "installCommand": "npm ci",
  "buildCommand": "npm run build"
}
