{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/*.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/generate",
      "dest": "/api/generate.py"
    },
    {
      "src": "/api/analyze-document",
      "dest": "/api/analyze.py"
    },
    {
      "src": "/((?!api/|assets/|.*\\..*).*)",
      "dest": "/index.html"
    }
  ],
  "framework": null,
  "installCommand": "npm ci",
  "buildCommand": "npm run build"
}
